    "/users/login": {
      "post": {
        "tags": ["users"],
        "summary": "Authentifie un utilisateur",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "email": { "type": "string" },
                  "password": { "type": "string" }
                },
                "required": ["email", "password"]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Utilisateur authentifié",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": { "type": "string" },
                    "token": { "type": "string" },
                    "hasSpa": { "type": "boolean" }
                  }
                }
              }
            }
          },
          "401": {
            "description": "Email ou mot de passe invalide"
          }
        }
      }
    },
